<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SalaryValidate_Acnt</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL( Designation__c , &apos;Accountant&apos;) , ISPICKVAL( Salary__c , &apos;10K&apos;))</errorConditionFormula>
    <errorDisplayField>Salary__c</errorDisplayField>
    <errorMessage>Please Ensure to select 15K for Accountant</errorMessage>
</ValidationRule>
