<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Checkbox_Error</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Checkbox_A__c,Checkbox_B__c)</errorConditionFormula>
    <errorMessage>Make sure both Checkboxes are not checked</errorMessage>
</ValidationRule>
